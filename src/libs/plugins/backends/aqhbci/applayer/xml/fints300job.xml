  <JOBdef id="JobDialogInit" version="3" pversion="300"
	internal="1" mode="RDH">
	<SEG type="Ident" name="ident" />
	<SEG type="Prepare" name="prepare" version="3" />
	<SEG type="getKey" name="cryptkey" />
	<SEG type="getKey" name="signKey" />
	<SEG type="getKey" name="authKey" />
	<VALUES>
		<VALUE path="cryptkey/key/keytype">V</VALUE>
		<VALUE path="cryptkey/key/keynum">?cryptkeynum</VALUE>
		<VALUE path="cryptkey/key/keyversion">?cryptkeyver</VALUE>
		<VALUE path="cryptkey/key/userid">?cryptkeyuserid</VALUE>
		<VALUE path="cryptKey/secprofile/code">RDH</VALUE>
		<VALUE path="cryptKey/secprofile/version">?rxhversion</VALUE>
		<VALUE path="signkey/key/keytype">S</VALUE>
		<VALUE path="signkey/key/keynum">?signkeynum</VALUE>
		<VALUE path="signkey/key/keyversion">?signkeyver</VALUE>
		<VALUE path="signkey/key/userid">?signkeyuserid</VALUE>
		<VALUE path="signKey/secprofile/code">RDH</VALUE>
		<VALUE path="signKey/secprofile/version">?rxhversion</VALUE>
		<VALUE path="authkey/key/keytype">D</VALUE>
		<VALUE path="authkey/key/keynum">?authkeynum</VALUE>
		<VALUE path="authkey/key/keyversion">?authkeyver</VALUE>
		<VALUE path="authkey/key/userid">?authkeyuserid</VALUE>
		<VALUE path="authKey/secprofile/code">RDH</VALUE>
		<VALUE path="authKey/secprofile/version">?rxhversion</VALUE>
	</VALUES>
</JOBdef>
 
