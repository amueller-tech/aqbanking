 </SEGdef>



 <SEGdef id="Prepare" 
         code="HKVVB" 
         version="2"
         pversion="201"
         delimiter="+" 
         terminator="'"
 >
   <GROUP type="SegHead" name="head" />
   <ELEM name="bpdVersion" type="num" maxsize="3" hide="1" topdown="1" />
   <ELEM name="updVersion" type="num" maxsize="3" hide="1" topdown="1" />
