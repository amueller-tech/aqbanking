

<types>
  <type id="AH_TAN_METHOD" 
        prefix="AH_TanMethod" 
        class="AH_TanMethod"
        access="public"
        filename="tanmethod"
        inherit="public"
        list="public"
        list2="public" >
    <descr>
      This group represents a TAN method within AqHBCI.
    </descr>

    <pre-headers>
      <header type="sys" >aqhbci/aqhbci.h</header>
    </pre-headers>

    <elem name="function" type="int" />
    <elem name="process" type="int" />
    <elem name="methodId" type="char" ptr="1" />
    <elem name="methodName" type="char" ptr="1" />
    <elem name="zkaTanName" type="char" ptr="1" />
    <elem name="zkaTanVersion" type="char" ptr="1" />
    <elem name="tanMaxLen" type="int" />
    <elem name="formatId" type="char" ptr="1" />
    <elem name="prompt" type="char" ptr="1" />
    <elem name="returnMaxLen" type="int" />
    <elem name="maxActiveLists" type="int" />
    <elem name="multiTanAllowed" type="int" />
    <elem name="timeShiftAllowed" type="int" />
    <elem name="tanListMode" type="int" />
    <elem name="stornoAllowed" type="int" />
    <elem name="needSmsAccount" type="int" />
    <elem name="needLocalAccount" type="int" />
    <elem name="needChallengeClass" type="int" />
    <elem name="needChallengeAmount" type="int" />
    <elem name="challengeIsStructured" type="int" />
    <elem name="initMode" type="int" />
    <elem name="needTanMediumId" type="int" />
    <elem name="maxActiveTanMedia" type="int" />
    <elem name="gvVersion" type="int" />
  </type>
  
  
  
</types>
