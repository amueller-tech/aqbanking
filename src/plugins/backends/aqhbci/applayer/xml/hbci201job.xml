 <!-- Geschaeftsvorfall: Statusprotokoll abrufen (Version 2) -->
 <JOBdef id="JobGetStatus"
    	   code="HKPRO"
         params="HIPROS"
         version="2"
 		     crypt="1"
         sign="1" 
         needBPD="0"
         attachable="1"
         response="GetStatusResponse"
  >
   <SEG type="GetStatus" version="2" />
   <VALUES>
     <VALUE path="head/code">%code</VALUE>
     <VALUE path="head/version">%version</VALUE>
   </VALUES>
 </JOBdef>



</JOBs>



